<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.javae.dao.AccountDao">
<!--    <cache type="com.javae.cache.RedisCache"/>-->
    <cache/>
    <select id="queryAll" resultType="com.javae.pojo.Account">
        select * from sm_account
    </select>
    <update id="updatePasswordById">
        update sm_account set password = #{password} where id = #{id}
    </update>

</mapper>